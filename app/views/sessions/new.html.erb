<div class="login">
  <%= image_tag('seicute_logo_login.png', size: '200x200', alt: "ロゴ") %>
</div>
<br><br>
<%= form_with(url: sessions_path, local: true) do |f| %>  
  <p><%= f.label :メールアドレス %></p>
  <p><%= f.email_field :email, style: "ime-mode:disabled" %></p>
  <p><%= f.label :パスワード %></p>
  <p><%= f.password_field :password %></p>
  <br>
  <%= f.submit "ログイン" %>
<% end %>
<br>
<p><%= link_to "ゲストログイン", 'guest_login', method: :post %></p>
<p><%= link_to "ゲストログイン(管理者)", 'guest_admin_login', method: :post %></p>